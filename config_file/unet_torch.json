        "test_bc":2,
        "begin_epoch": 0,
        "end_epoch": 100,
        "early_stop": 30,
        "optimizer": "adamw",
        "lr": 0.001,
        "wd": 5e-4,
        "momentum": 0.0,
        "nesterov": false 
    }
